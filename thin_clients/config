[server]
address=http://192.168.0.20/kvm-vdi/

[loggers]
keys=root

[handlers]
keys=VDIconsoleHandler

[formatters]
keys=VDIformatter

[logger_root]
handlers=VDIconsoleHandler
level=INFO

[handler_VDIconsoleHandler]
class=StreamHandler
level=INFO
formatter=VDIformatter
args=(sys.stderr,)
[formatter_VDIformatter]
format=%(asctime)s %(name)-12s %(levelname)-8s %(message)s